{
  "extends": ["standard"],
  "env": {
    "jest": true,
  },
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "import",
  ],
  "rules": {
    "import/no-unresolved": ["error"],
    "comma-dangle": ["error", "only-multiline"],
    "object-curly-spacing": ["error", "never"],
    "yoda": ["off"],
  }
}
